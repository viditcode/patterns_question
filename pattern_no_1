#include <bits/stdc++.h>

using namespace std;
//class for solution.
class Solution {
  public:
    void printSquare(int n) {
        // code here
        int i;
        for (i=0; i<n; i++){
            for (int j =0; j<n; j++){
                cout << "*";
                cout <<" ";
            }
            cout<<endl;
        }
    }
};

/main function

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;

        Solution ob;
        ob.printSquare(n);
    }
    return 0;
 }
