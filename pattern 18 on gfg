#include <bits/stdc++.h>

using namespace std;

// } Driver Code Ends
class Solution {
  public:
    void printTriangle(int n) {
        // code here
        //char ch='E ';
        for (int i =0 ; i < n; i++ ){
                   char ch='E';
            for (int j =0; j<=i ; j++){
                cout <<ch <<" ";
                ch=ch-1;
        
               
            }
            cout <<endl;
        }
    }
};

//{ Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;

        Solution ob;
        ob.printTriangle(n);
    }
    return 0;
}
