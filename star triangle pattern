#include <bits/stdc++.h>

using namespace std;

// } Driver Code Ends

//solution for triangle paatern of star
class Solution {
  public:
    void printSquare(int n) {
        // code here
        int i;
      for (int i=0; i<n;i++){
	        for (int j=0; j<i+1;j++){
	            cout <<"*"<<" ";
	        }
            cout<<endl;
        }
    }
};

//{ Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;

        Solution ob;
        ob.printSquare(n);
    }
    return 0;
